package TUGAS_ASD3;

import java.util.ArrayList;

public class QueueFloat {
	ArrayList<Float> kelase = new ArrayList<Float>();
	int abrar = -1;

	public void insert(Float value) {
		ismail++;
		kelase.add(abrar,value);
	}
	public Float get() {
		Float value = 0.1f;
		if(abrar>-1) {
			value = kelase.get(0);
			kelase.remove(0);
			ismail--;
		}
		return value;
		
	}
	public void cetak() {
		System.out.println("ECOR : "+abrar+" > VARIABLE: "+kelase.toString());
	}
}
